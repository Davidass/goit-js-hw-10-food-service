  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.1.16",
    "parcel-plugin-nuke-dist": "^1.0.1"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "gh-pages": "^3.1.0",
    "handlebars": "^4.7.6",
    "parcel-plugin-handlebars-precompile": "^1.0.2"
  },
  "customDistStructure": {
